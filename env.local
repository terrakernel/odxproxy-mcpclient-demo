# Gemini
GEMINI_API_KEY=your_gemini_key_here

# How to spawn the local MCP server (same as Claude "command + args")
MCP_SERVER_COMMAND=node
MCP_SERVER_ENTRY=/ABSOLUTE/PATH/odxproxy-mcpserver/dist/index.js
MCP_SERVER_ARGS=

# If your MCP server uses these (common for odxproxy-mcpserver):
ODX_INSTANCE_URL=https://your-odoo-host
ODX_INSTANCE_DB=your_db
ODX_INSTANCE_USER_ID=2
ODX_INSTANCE_API_KEY=xxxxx
ODXPROXY_API_KEY=xxxxx
ODX_GATEWAY_URL=https://gateway.odxproxy.io

DEBUG_MCP=1
MCP_TOOL_NAME=send_text_to_backend